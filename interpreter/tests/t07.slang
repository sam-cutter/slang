// Greater than
print(1 > 2);
print(2.0 > 1.0);
print("Hello" > "World");

// Less than
print(10 < 20);
print(1 * 10^6 < 1 * 10^5);
print("Computer" < "Science");

// Greater than or equal to
print(10 >= 20);
print(2.0 >= 2.0);
print("Hello" >= "World");

// Less than or equal to
print(-100 <= 10 * -10);
print(1 * 10^6 <= 1 * 10^2^2);
print("racecar" <= "racecar");